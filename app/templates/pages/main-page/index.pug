extends ../../template/main-page-new

block body
  main
    +banner({
      link: '../img/background-main.png',
      items: [{
        bannerMods: ['no-opacity', 'flex'],
      }]
    })
      +title({
      mods: ['banner'],
      text: '<p>IFK Hotel Management – управляющая компания <span class="text-red">для гостиниц</span></p>'
      })
      .desc За 10 лет мы накопили большой опыт в управлении бизнесом который поможет добиться успеха вашему бизнесу
      +button({
        mods: ['red'],
        text: ['Отправить заявку']
      })
