  .callback-form
      .container
        +callback({
          title: 'Как мы это делаем?',
          policy: 'Нажимая на кнопку “Отправить” вы соглашаетесь на обработку персональных данных',
          wraps: [{
            mods: ['big'],
            inputs: [{
                mods: ['big'],
                placeholder: 'Опишите ваши пожелания',
                tag: 'textarea'
              }]
            },
            { mods: ['small'],
